{
  "name": "nftao",
  "version": "0.2.0",
  "private": false,
  "scripts": {
    "api": "nx run backend:serve:production",
    "build": "nx run-many --target=build --projects=contracts,backend,frontend --parallel=100",
    "build:frontend": "nx build frontend",
    "hardhat": "npx hardhat node",
    "hardhat:deploy": "HARDHAT_NETWORK=localhost npx hardhat run libs/contracts/src/scripts/deploy.js",
    "hardhat:deploy:goerli": "HARDHAT_NETWORK=goerli npx hardhat run libs/contracts/src/scripts/deploy.js",
    "hardhat:deploy:sepolia": "HARDHAT_NETWORK=sepolia npx hardhat run libs/contracts/src/scripts/deploy.js",
    "hardhat:verify:goerli": "HARDHAT_NETWORK=goerli npx hardhat run libs/contracts/src/scripts/verify.js",
    "hardhat:verify:sepolia": "HARDHAT_NETWORK=sepolia npx hardhat run libs/contracts/src/scripts/verify.js",
    "lint": "nx exec -- eslint apps/frontend/src/frontend",
    "prepare": "husky install",
    "start": "nx serve frontend",
    "start:all": "concurrently --names \"HARDHAT,API,FRONTEND\" -p \"[{name}:{time}]\" -t \"HH:mm:ss\" -c auto --kill-others \"pnpm hardhat\" \"nx serve backend\" \"nx serve frontend\"",
    "start:api": "nx run backend:serve:development"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write ."
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "dependencies": {
    "@chainlink/contracts": "^0.4.2",
    "@chakra-ui/react": "^2.5.3",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nrwl/eslint-plugin-nx": "15.8.9",
    "@nrwl/nx-cloud": "15.3.3",
    "@nrwl/vite": "15.8.9",
    "@nrwl/webpack": "15.8.9",
    "@nrwl/workspace": "15.8.9",
    "@openzeppelin/contracts": "^4.8.2",
    "@reduxjs/toolkit": "^1.9.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/express": "~4.17.13",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.0.1",
    "body-parser": "^1.20.1",
    "chai": "^4.3.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "7.32.2",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "framer-motion": "^10.9.1",
    "hardhat": "^2.12.2",
    "husky": "^8.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^7.0.1",
    "multer": "1.4.5-lts.1",
    "nx": "15.8.9",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.6.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-is": "18.2.0",
    "react-outclick": "^0.0.4",
    "react-redux": "^8.0.5",
    "react-router": "^6.4.3",
    "react-router-dom": "6.8.1",
    "react-share": "^4.4.1",
    "reselect": "^4.1.7",
    "styled-components": "^5.3.6",
    "tslib": "^2.3.0",
    "typescript": "~4.9.5",
    "uuid": "^9.0.0",
    "vite": "^4.0.4",
    "vite-plugin-dts": "~1.7.1",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.7",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
