{
  "name": "starter_kit",
  "version": "0.2.0",
  "private": false,
  "dependencies": {
    "@chainlink/contracts": "^0.4.2",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@openzeppelin/contracts": "^4.8.2",
    "@reduxjs/toolkit": "^1.9.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@vitejs/plugin-react": "^3.0.1",
    "body-parser": "^1.20.1",
    "chai": "^4.3.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.11",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "hardhat": "^2.12.2",
    "husky": "^8.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^7.0.1",
    "multer": "1.4.5-lts.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.6.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.7.1",
    "react-outclick": "^0.0.4",
    "react-redux": "^8.0.5",
    "react-router": "^6.4.3",
    "react-router-dom": "^6.4.3",
    "react-share": "^4.4.1",
    "reselect": "^4.1.7",
    "styled-components": "^5.3.6",
    "uuid": "^9.0.0",
    "vite": "^4.0.4",
    "vite-plugin-svgr": "^2.4.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "prepare": "husky install",
    "hardhat": "npx hardhat node",
    "hardhat:deploy": "HARDHAT_NETWORK=localhost npx hardhat run src/backend/scripts/deploy.js",
    "hardhat:deploy:goerli": "HARDHAT_NETWORK=goerli npx hardhat run src/backend/scripts/deploy.js",
    "api": "nodemon --watch .env --watch src/backend --ignore src/frontend --ignore assets src/backend/api/server.js",
    "api:dev": "nodemon --inspect --watch .env --watch src/backend --ignore src/frontend --ignore assets src/backend/api/server.js",
    "start": "vite",
    "start:all": "concurrently --names \"HARDHAT,API,FRONTEND\" -c auto --kill-others \"pnpm hardhat\" \"pnpm api:dev\" \"pnpm start\"",
    "build": "vite build",
    "lint": "eslint src/frontend"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write ."
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/mocha": "^9.1.0",
    "concurrently": "^7.6.0",
    "eslint": "8.22.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "hardhat-ethernal": "^3.1.0",
    "hardhat-gas-reporter": "^1.0.8",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.21",
    "prettier": "2.7.1",
    "solidity-coverage": "^0.8.1",
    "web3.storage": "^4.4.0"
  }
}
