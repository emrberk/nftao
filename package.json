{
  "name": "starter_kit",
  "version": "0.2.0",
  "private": false,
  "dependencies": {
    "@babel/core": "7.13.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@chainlink/contracts": "^0.4.2",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.8.0",
    "@reduxjs/toolkit": "^1.9.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "body-parser": "^1.20.1",
    "chai": "^4.3.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.11",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "hardhat": "^2.12.2",
    "husky": "^8.0.2",
    "lodash": "^4.17.21",
    "multer": "1.4.5-lts.1",
    "mysql-migrations": "^1.0.7",
    "mysql2": "^2.3.3",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.6.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.7.1",
    "react-redux": "^8.0.5",
    "react-router": "^6.4.3",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.0",
    "reselect": "^4.1.7",
    "styled-components": "^5.3.6",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "prepare": "husky install",
    "hardhat": "npx hardhat node",
    "hardhat:deploy": "npx hardhat run src/backend/scripts/deploy.js --network localhost",
    "api": "nodemon --ignore src/frontend --ignore assets src/backend/api/server.js",
    "start": "react-scripts start",
    "start:all": "concurrently --names \"HARDHAT,API,FRONTEND\" -c auto --kill-others \"pnpm hardhat\" \"pnpm api\" \"pnpm start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/frontend"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write .",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@babel/helper-compilation-targets": "^7.20.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "concurrently": "^7.6.0",
    "eslint": "8.22.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20",
    "prettier": "2.7.1",
    "web3.storage": "^4.4.0"
  }
}
